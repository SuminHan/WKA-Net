{
  "embed_dim": 64,
  "num_layers": 3,
  "num_heads": 2,
  "TG_in_one_day": 24,
  "forward_expansion": 4,
  "dropout_rate": 0,

  "scaler": "standard",
  "load_external": true,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": true,
  "add_day_in_week": true,

  "max_epoch": 100,
  "learner": "adam",
  "learning_rate": 1e-3,
  "lr_decay": false,
  "clip_grad_norm": false,
  "max_grad_norm": 5,
  "use_early_stop": true,
  "patience": 50
}
